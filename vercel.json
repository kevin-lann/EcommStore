{
    "rewrites" : [
      {
        "source" : "/backend/(.*)",
        "destination": "/api"
      }
    ],
    "version": 2,
    "builds": [
      
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }